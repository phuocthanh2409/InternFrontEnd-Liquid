 <nav role="navigation">
    <ol class="pagination">
      {%- if paginate.previous-%}
        <a href="{{ paginate.previous.url }}">
          <li>
            <span aria-hidden="true">
              <svg width="24" height="24" viewBox="0 0 24 24" fill="none" xmlns="http://www.w3.org/2000/svg">
              <path d="M13.36 17C13.2253 16.9995 13.0921 16.9718 12.9684 16.9185C12.8446 16.8652 12.7329 16.7875 12.64 16.69L8.78 12.69C8.59677 12.5031 8.49414 12.2518 8.49414 11.99C8.49414 11.7282 8.59677 11.4769 8.78 11.29L12.78 7.29C12.8732 7.19676 12.9839 7.1228 13.1057 7.07234C13.2276 7.02188 13.3581 6.99591 13.49 6.99591C13.6219 6.99591 13.7524 7.02188 13.8742 7.07234C13.9961 7.1228 14.1068 7.19676 14.2 7.29C14.2932 7.38324 14.3672 7.49393 14.4177 7.61575C14.4681 7.73758 14.4941 7.86814 14.4941 8C14.4941 8.13186 14.4681 8.26243 14.4177 8.38425C14.3672 8.50607 14.2932 8.61676 14.2 8.71L10.9 12L14.08 15.3C14.2662 15.4874 14.3708 15.7408 14.3708 16.005C14.3708 16.2692 14.2662 16.5226 14.08 16.71C13.9854 16.8039 13.8729 16.8779 13.7493 16.9277C13.6256 16.9775 13.4933 17.0021 13.36 17Z" fill="#14171A"/>
              </svg>
            </span>
            <span class="visuallyhidden">page</span>
          </li>
        </a>
      {%- else -%}
        <li class="disabled">
          <span aria-hidden="true">
          </span>
          <span class="visuallyhidden">page</span>
        </li>
      {%- endif -%}

      {%- for part in paginate.parts -%}
        {%- if part.is_link -%}
          <a href="{{ part.url }}">
            <li class="page-disabled">
              <span class="visuallyhidden">page</span> {{ part.title }}
            </li>
          </a>
        {%- else -%}
          {%- if part.title == paginate.current_page -%}
            <li class="active" aria-current="page">
              <span class="visuallyhidden">page</span> {{ part.title }}
            </li>
          {%- else -%}
            <li class="page-disabled">
              <span class="visuallyhidden">page</span> {{ part.title }}
            </li>
          {%- endif -%}
        {%- endif -%}
      {%- endfor -%}

      {%- if paginate.next -%}
        <a href="{{ paginate.next.url }}">
          <li>
            <span class="visuallyhidden">page</span>
            <span aria-hidden="true">
              <svg width="24" height="24" viewBox="0 0 24 24" fill="none" xmlns="http://www.w3.org/2000/svg">
              <path d="M10.4999 17C10.3683 17.0008 10.2379 16.9755 10.116 16.9258C9.9942 16.876 9.88338 16.8027 9.78994 16.71C9.69621 16.617 9.62182 16.5064 9.57105 16.3846C9.52028 16.2627 9.49414 16.132 9.49414 16C9.49414 15.868 9.52028 15.7373 9.57105 15.6154C9.62182 15.4936 9.69621 15.383 9.78994 15.29L13.0999 12L9.91994 8.68999C9.73369 8.50263 9.62915 8.24918 9.62915 7.98499C9.62915 7.7208 9.73369 7.46735 9.91994 7.27999C10.0129 7.18626 10.1235 7.11187 10.2454 7.0611C10.3672 7.01033 10.4979 6.98419 10.6299 6.98419C10.762 6.98419 10.8927 7.01033 11.0145 7.0611C11.1364 7.11187 11.247 7.18626 11.3399 7.27999L15.1999 11.28C15.3832 11.4669 15.4858 11.7182 15.4858 11.98C15.4858 12.2417 15.3832 12.4931 15.1999 12.68L11.1999 16.68C11.1102 16.7769 11.0021 16.8551 10.882 16.91C10.7618 16.965 10.632 16.9955 10.4999 17Z" fill="#14171A"/>
              </svg>
            </span>
          </li>
        </a>
      {%- else -%}
        <li class="disabled">
          <span class="visuallyhidden">page</span>
          <span aria-hidden="true">
          </span>
        </li>
      {%- endif -%}
    </ol>
  </nav>
  <style>
.visuallyhidden {
  border: 0;
  clip: rect(0 0 0 0);
  height: 1px;
  margin: -1px;
  overflow: hidden;
  padding: 0;
  position: absolute;
  width: 1px;
  white-space: nowrap;
}
.pagination{
  text-align: center;
  display: flex;
  justify-content: center;
  align-items: center;
}
.pagination a{
  text-decoration: none;
  color: #BABCBF;
}
.pagination li {
  margin: 0 5px;
}
.active{
  width: 30px;
  height: 30px;
  background: #E94141;
  border-radius: 0px;
  display: flex;
  justify-content: center;
  align-items: center;
  font-style: normal;
  font-weight: normal;
  font-size: 12px;
  line-height: 14px;
  text-align: center;
  color: #FFFFFF;
}
.page-disabled{
  width: 30px;
  height: 30px;
  border: 2px solid #F6F6F6;
  box-sizing: border-box;
  border-radius: 0px;
  display: flex;
  justify-content: center;
  align-items: center;
  font-style: normal;
  font-weight: normal;
  font-size: 12px;
  line-height: 14px;
  text-align: center;
  color: #BABCBF;
}
</style>